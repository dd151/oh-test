<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Sanity-Suite" parallel="tests" thread-count="5">
	<groups>
		<run>
			<include name="Smoke" />
		</run>
	</groups>
	<listeners>
		<listener class-name="com.ohrm.listeners.TestListener"></listener>
	</listeners>
	<test name="Login">
		<classes>
			<class name="com.ohrm.tests.LoginTest" />
		</classes>
	</test>
	<test name="Logout">
		<classes>
			<class name="com.ohrm.tests.LogoutTest" />
		</classes>
	</test>
</suite>